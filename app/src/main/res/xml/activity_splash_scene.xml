<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/image2"
        motion:constraintSetStart="@id/image3"
        motion:duration="1000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/imageView2"
               motion:framePosition="20"
               android:scaleX="0.4"
               android:scaleY="0.4" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView2"
               motion:framePosition="50"
               android:scaleX="0.4"
               android:scaleY="0.4" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView2"
               motion:framePosition="100"
               android:scaleX="60"
               android:scaleY="60" />
       </KeyFrameSet>
        <OnClick motion:targetId="@+id/imageView2" />
    </Transition>

    <ConstraintSet android:id="@+id/image3">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/image2">
    </ConstraintSet>
</MotionScene>